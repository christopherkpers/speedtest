{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://localhost:80/speedtest/ruralfone.html"
            //"webRoot": "${workspaceFolder}"
        },
    {
        "name": "Listen for Xdebug",
        "type": "php",
        "request": "launch",
        "port": 9000,
        "pathMappings": {
            "/vagrant": "${workspaceFolder}",
        }
    },
        {
            "name": "Launch currently open script",
            "type": "php",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "port": 9000
        }
    ]
}